<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.FlowPane?>

<FlowPane alignment="CENTER" columnHalignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
          minWidth="-Infinity" orientation="VERTICAL" prefHeight="640.0" prefWidth="960.0" styleClass="background"
          xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="sample.MenuController">
    <opaqueInsets>
        <Insets/>
    </opaqueInsets>
    <stylesheets>
        <URL value="@../css/menu.css"/>
    </stylesheets>
    <Button fx:id="mainStart" alignment="TOP_LEFT" mnemonicParsing="false" onAction="#renderWithBot"
            styleClass="button" text="Start game">
        <opaqueInsets>
            <Insets top="80.0"/>
        </opaqueInsets>
    </Button>
    <Button fx:id="mainNet" mnemonicParsing="false" onAction="#renderWithUser" styleClass="button"
            text="Network mode"/>
</FlowPane>
